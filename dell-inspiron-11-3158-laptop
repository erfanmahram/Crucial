<!--        <script type="text/javascript">
        var wait;
        var searchinput;
        var search_ga_event_wait;

        $(document).ready(function(){

            var isMobile = $('body').hasClass('is-mobile');
            var close_events = isMobile ? 'click, touchstart' : 'click';
            $(document).on(close_events, '[data-smartsearch-results]', function(evt){
                evt.stopPropagation();
            });
            $(document).on(close_events, '.off-canvas-wrap', function(){
                hideSmartSearch();
            });

            $('#search input').after('<div class="smartsearch absolute top-100 left-0 width-100 text-center" style="display: none;"></div>').blur(function(){
                clearTimeout(wait);
            }).keydown(function(e){
                if ($('.smartsearch-product').length && e.which == 38) {
                    e.preventDefault();
                    return false;
                }
            }).keyup(function(e){
                searchinput = $(this);
                if (!searchinput.val()) {
                    clearTimeout(wait);
                    wait = setTimeout(hideSmartSearch, 500);
                }
                if (e.which == 13 && $('.smartsearch-focus').length) {
                    location = $('.smartsearch-focus').attr('href');
                }
                if (searchinput.val().replace(/^\s+|\s+$/g, '') && (e.which == 8 || (47 < e.which && e.which < 112) || e.which > 185)) {
                    clearTimeout(wait);
                    showSmartSearchLoader();
                    wait = setTimeout(showSmartSearch, 500);
                }
                if ($('.smartsearch-product').length && (e.which == 38 || e.which == 40)) {
                    if (!$('.smartsearch-focus').length) {
                        if (e.which == 38) $('.smartsearch-bottom').prev().addClass('smartsearch-focus');
                        if (e.which == 40) $('.smartsearch-product:first-child').addClass('smartsearch-focus');
                    } else {
                        if (e.which == 38) $('.smartsearch-focus').removeClass('smartsearch-focus').prev('a').addClass('smartsearch-focus');
                        if (e.which == 40) $('.smartsearch-focus').removeClass('smartsearch-focus').next('a').addClass('smartsearch-focus');
                    }
                }
            });

            $(document).on('click', '[data-suggested-search-term]', function(evt){
                evt.preventDefault();

                searchinput.val( $(this).data('term') );
                hideSmartSearch();
                showSmartSearchLoader();
                showSmartSearch();
            });
        });

        function showSmartSearchLoader(){
            var search_text = 'Searching<span class="hide-small-only"> Products &amp; Devices</span>...';
            var search_loader = '<i class="fi fi-loading spin-cw colour-grey-dark margin-r10 margin-r20-large-up block line-height-1"></i>';

            var smart_search_loader = '';
            smart_search_loader += '<div class="smartsearch-shadow small-shadow relative background colour-grey-lightest inline-block width-100 width-auto-large-up margin-t10 radius">';
            smart_search_loader += '    <div class="width-100 padding-25 padding-x50-large-up text-center radius" style="border: 2px solid #888;">';
            smart_search_loader += '        <div class="inline-block vertical-middle margin-t5">';
            smart_search_loader += '        <div class="table">';
            smart_search_loader += '            <span class="table-cell vertical-middle">';
            smart_search_loader += '                <span class="font-size-50 hide-medium-up">' + search_loader + '</span>';
            smart_search_loader += '                <span class="font-size-32 hide-small-only">' + search_loader + '</span>';
            smart_search_loader += '            </span>';
            smart_search_loader += '            <span class="table-cell vertical-middle font-weight-600 colour-black text-left">';
            smart_search_loader += '                <span class="font-size-24 hide-medium-up">' + search_text + '</span>';
            smart_search_loader += '                <span class="font-size-18 hide-small-only">' + search_text + '</span>';
            smart_search_loader += '            </span>';
            smart_search_loader += '        </div>';
            smart_search_loader += '        </div>';
            smart_search_loader += '    </div>';
            smart_search_loader += '</div>';
            searchinput.next().html( smart_search_loader ).show();
        }

        function showSmartSearch() {
            var container = $('#search input').parent();
            var cat = $('#filter_category_id').val();

            $.ajax({
                url: 'index.php?route=module/smartsearch/smartsearch&search=' + searchinput.val() + '&category_id=' + cat,
                dataType: 'json',
                success: function(data) {
                    var products = data.products;
                    var devices = data.devices;
                    var categories = data.categories;
                    var manufacturers = data.manufacturers;
                    var series = data.series;

                    // We need to construct the results based on what results we actually have, so split the HTML
                    // building into multiple variables so we can swap and change them.
                    var results_open = '';
                    var results_close = '';
                    var left_column_open = '';
                    var left_column_close = '';
                    var right_column_open = '';
                    var right_column_close = '';
                    var device_html = '';
                    var category_html = '';
                    var product_html = '';

                    var left_column_width_class = products.length ? 'large-8' : 'large-6';
                    var right_column_width_class = products.length ? 'large-4' : 'large-6';
                    var dropdown_type = products.length ? 'multi-result-' : 'single-result-';

                    // Section BG Colours
                    var bgColor;
                    if (categories.length == 0){
                        bgColor = "#e6e6e6";
                    } else {
                        bgColor = "#dcdcdc";
                    }

                    // Set the result wrapper open HTML
                    results_open += '<div data-smartsearch-results class="smartsearch-shadow ' + dropdown_type + ' full-shadow relative width-100 width-auto-large-up margin-t10 text-left radius" style="background-color: ' + bgColor + '; ">';
                    results_open += '<div class="radius" style="border: 2px solid #888">';
                    results_open += '<div class="row width-auto radius">';

                    // Set the left column open HTML
                    left_column_open += '<div class="smartsearch-results-product-column results-left-column column padding-r0-large-up ' + left_column_width_class + '" data-mh="header-smartsearch">';
                    left_column_open += '               <div class="height-100 background colour-grey-lightest">';

                    // Start products listing
                    product_html += '                   <div class="product-results padding-20">';

                    product_html += '                   <div class="table width-100 margin-0 margin-b15">';
                    product_html += '                       <span class="table-cell width-100">';
                    product_html += '                           <h3 class="block font-size-18 fixed-font-size font-weight-600 colour-grey-darkest margin-0">Products</h3>';
                    product_html += '                       </span>';

                    if( products.length ){
                        product_html += '                   <span class="table-cell width-100">';
                        product_html += '                       <a href="https://www.memorycow.co.uk/index.php?route=product/search&category_id=' + $('#filter_category_id').val() + '&description=1&model=1&show_products=1&keyword=' + encodeURIComponent(searchinput.val()) + '" class="colour-blue underline font-size-14 nowrap">View all Products</a>';
                        product_html += '                   </span>';
                    }

                    product_html += '                   </div>';
                    // End products heading

                    // Start products listing
                    if( products.length ){
                        product_html += '               <div class="width-100 margin-b0">';

                        for( var i = 0; i < products.length; i++ ){

                            var product = products[i];
                            product_html += '               <a href="' + product.href + '&filter_name=' + searchinput.val() + '" class="block colour-grey">';
                            product_html += '                   <div class="table width-100 padding-y5 colour-grey-dark">';

                            // Image Cell
                            if( typeof( product.thumb ) != 'undefined' ) {
                                product_html += '                       <div class="table-cell vertical-top">';
                                product_html += '                       <img src="' + product.thumb + '" class="block border-1 colour-grey-light margin-r10" title="View Product" style="max-width: ' + (product.thumb_width / 2) + 'px;" />';
                                product_html += '                       </div>';
                            }

                            // Details Cell
                            product_html += '                       <div class="table-cell width-100 vertical-top font-weight-400">';

                            // Name
                            product_html += '                           <div class="table width-100">';

                            product_html += '                               <div class="table-cell vertical-top">';
                            product_html += '                                   <span class="block colour-grey-darkest font-size-15 line-height-1pt3 underline-on-hover">' + product.name + '</span>';

                            // Model (Desktop)
                            if( typeof( product.model ) != 'undefined' ){
                                product_html += '                               <span class="none block-large-up font-size-13 colour-grey-darkest opacity-6 underline-on-hover margin-t4">' + product.model + '</span>';
                            }

                            product_html += '                               </div>';

                            // Price (Desktop)
                                                        if( typeof( product.price ) != 'undefined' ){

                                var price = '<span class="" style="' + (product.special ? 'text-decoration: line-through' : '') + '">' + product.price + '</span>';
                                var special = (product.special ? '<span>' + product.special + '</span>' : '');

                                product_html += '<div class="table-cell vertical-top text-right padding-l20-large-up">';
                                product_html += '   <div class="none block-large-up font-size-18 colour-off-black font-weight-600 line-height-1pt3">' + price + ' ' + special + '</div>';
                                product_html += '   <div class="none block-large-up font-size-12 font-weight-400 colour-grey-dark">inc. VAT</div>';
                                product_html += '</div>';

                            }

                            product_html += '                           </div>';

                            // Model
                            product_html += '                           <span class="table width-100">';
                            if( typeof( product.model ) != 'undefined' ){
                                product_html += '                               <span class="table-cell width-100 font-size-13 fixed-font-size underline-on-hover"><span class="hide-large-up">' + product.model + '</span></span>';
                            }

                            // Price (Mobile / Tablet)
                                                        if( typeof( product.price ) != 'undefined' ){
                                var price = '<span class="" style="' + (product.special ? 'text-decoration: line-through' : '') + '">' + product.price + '</span>';
                                var special = (product.special ? '<span>' + product.special + '</span>' : '');
                                var vat = '<span class="none inline-block-large-up font-size-12 font-weight-400 colour-grey-dark">inc. VAT</span>';
                                product_html += '                       <span class="table-cell hide-large-up text-right"><span class="nowrap font-weight-600 colour-off-black">' + price + ' ' + special + vat + '</span></span>';
                            }

                            product_html += '                           </span>';
                            product_html += '                       </div>';
                            product_html += '                   </div>';
                            product_html += '               </a>';

                                                        if (i !== (products.length - 1)){
                                product_html += '<hr class="margin-y15" />';
                            }


                        }

                        // View all matching products button
                        product_html += '               <div class="margin-t30 margin-b10 text-center">';
                        product_html += '                   <a href="https://www.memorycow.co.uk/index.php?route=product/search&category_id=' + $('#filter_category_id').val() + '&description=1&model=1&show_products=1&keyword=' + encodeURIComponent(searchinput.val()) + '" class="inline-block button background colour-grey radius padding-15 padding-x30 margin-0">';
                        product_html += '                       <span class="colour-grey-darkest font-weight-600">View all Products</span>';
                        product_html += '                   </a>';
                        product_html += '               </div>';

                        product_html += '           </div>';
                    } else {
                        product_html += '   <h4 class="font-size-18 font-weight-400 margin-t5-negative">Sorry, no matching products were found.</h4>';
                        product_html += '   <p class="font-weight-400 font-size-16 colour-grey-darkest opacity-8">- Have you tried searching for a device, category or keyword instead?<br/>- Have you checked your spelling?</p>';
                                                                        var suggested_terms_string = 'RAM;Memory;USB;HyperX;microSD';
                        var suggested_term_words = suggested_terms_string.split(';');
                        var suggested_terms = [];
                        for( var i = 0; i < suggested_term_words.length; i++ ){
                            var term = suggested_term_words[ i ];
                            var term_link = '<a data-suggested-search-term data-term="' + term + '" href="https://www.memorycow.co.uk/index.php?route=product/search&category_id=' + $('#filter_category_id').val() + '&description=1&model=1&show_products=1&keyword=' + encodeURIComponent( term ) + '" class="colour-grey-darkest underline-on-hover">' + term + '</a>'
                            suggested_terms.push( term_link );
                        }

                        product_html += '   <h4 class="margin-t20 margin-b0 font-weight-600">People are searching for:</h4>';
                        product_html += '   <p class="font-size-16 margin-0">' + suggested_terms.join( ' - ' ) + '</p>';
                                            }

                    product_html += '</div>';

                    // Set the left column close HTML
                    left_column_close += '           </div>';
                    left_column_close += '           </div>';

                    // Set the right column open HTML
                    right_column_open += '           <div class="column ' + right_column_width_class + ' padding-l0-large-up" data-mh="header-smartsearch">';
                    right_column_open += '               <div class="height-100">';

                    // Start devices

                    var deviceBG = '#ededed';
                    if ( products.length ){
                        deviceBG = '#dcdcdc';
                    }

                    device_html += '<div class="device-results padding-20" style="background-color: ' + deviceBG + ';">';

                    // Start devices heading
                    device_html += '               <div class="table width-100 margin-0 margin-b15">';
                    device_html += '                   <span class="table-cell width-100">';
                    device_html += '                       <h3 class="block font-size-18 fixed-font-size font-weight-600 colour-grey-darkest margin-0">Devices</h3>';
                    device_html += '                   </span>';
                    if( devices.length ){
                        device_html += '               <span class="table-cell width-100">';
                        device_html += '                   <a href="https://www.memorycow.co.uk/index.php?route=product/search&category_id=' + $('#filter_category_id').val() + '&description=1&model=1&show_devices=1&keyword=' + encodeURIComponent(searchinput.val()) + '" class="colour-blue underline font-size-14 nowrap">View all Devices</a>';
                        device_html += '               </span>';
                    }
                    device_html += '               </div>';
                    // End devices heading

                    // Start devices listing
                    if( devices.length ) {
                        device_html += '           <div class="width-100">';

                        for( var i = 0; i < devices.length; i++ ){
                            var device = devices[i];
                            device_html += '           <a href="' + device.href + '&filter_name=' + searchinput.val() + '" class="block margin-b5 colour-grey-dark">';
                            device_html += '               <div class="table width-100">';

                            // Image Cell
                            if( typeof( device.thumb ) != 'undefined' ) {
                                device_html += '                       <div class="table-cell vertical-middle">';
                                device_html += '                       <img src="' + device.thumb + '" class="block border-1 colour-grey margin-r10" style="max-width: ' + (device.thumb_width / 2) + 'px;" />';
                                device_html += '                       </div>';
                            }

                            if( !products.length && device.type_name != 'null' && device.type_name != null && false ){
                                device.name += ' ' + device.type_name;
                            }

                            // Name Cell
                            device_html += '                   <div class="table-cell width-100 vertical-middle">';
                            device_html += '                       <span class="colour-grey-darkest underline-on-hover">' + device.name + '</span>';
                            device_html += '                   </div>';

                            device_html += '               </div>';
                            device_html += '           </a>';
                        }

                        // View x other devices link...
                        if( data.other_devices ){
                            device_html += '           <div class="margin-t10">';
                            device_html += '               <a href="https://www.memorycow.co.uk/index.php?route=product/search&category_id=' + $('#filter_category_id').val() + '&description=1&model=1&show_devices=1&keyword=' + encodeURIComponent(searchinput.val()) + '" class="colour-grey-darkest underline">+ ' + data.other_devices + ' more devices</a>';
                            device_html += '           </div>'
                        }

                        device_html += '           </div>';
                    } else {
                        if( manufacturers.length || series.length ){
                            device_html += '            <div class="width-100">';

                            for( var i = 0; i < manufacturers.length; i++ ){
                                var manufacturer = manufacturers[i];
                                device_html += '        <a href="' + manufacturer.href + '&filter_name=' + searchinput.val() + '" class="block margin-b5 colour-grey-dark">';
                                device_html += '            <div class="table width-100">';
                                device_html += '                <div class="table-cell width-100 vertical-middle">';
                                device_html += '                    <span class="colour-grey-darkest underline-on-hover">' + manufacturer.name + '</span>';
                                device_html += '                </div>';
                                device_html += '            </div>';
                                device_html += '        </a>';
                            }

                            for( var i = 0; i < series.length; i++ ){
                                var series_item = series[i];
                                device_html += '        <a href="' + series_item.href + '&filter_name=' + searchinput.val() + '" class="block margin-b5 colour-grey-dark">';
                                device_html += '            <div class="table width-100">';
                                device_html += '                <div class="table-cell width-100 vertical-middle">';
                                device_html += '                    <span class="colour-grey-darkest underline-on-hover">' + series_item.name + '</span>';
                                device_html += '                </div>';
                                device_html += '            </div>';
                                device_html += '        </a>';
                            }

                            device_html += '            </div>';
                        } else {
                            device_html += '    <p class="margin-t10-negative margin-b10 font-size-18 colour-grey-darkest">Sorry, no matching devices were found.</p>';
                        }
                    }
                    // End devices listing

                    device_html += '        </div>';

                    // End devices

                    // Start categories

                    category_html += '               <div class="category-results padding-20 background colour-grey-light">';

                    // Start categories heading
                    category_html += '               <div class="table width-100 margin-0 margin-b15">';
                    category_html += '                   <span class="table-cell width-100">';
                    category_html += '                       <h3 class="block font-size-18 fixed-font-size font-weight-600 colour-grey-darkest margin-0">Categories</h3>';
                    category_html += '                   </span>';
                    if( categories.length ){
                        category_html += '               <span class="table-cell width-100">';
                        category_html += '                   <a href="https://www.memorycow.co.uk/index.php?route=product/search&category_id=' + $('#filter_category_id').val() + '&description=1&model=1&show_categories=1&keyword=' + encodeURIComponent(searchinput.val()) + '" class="colour-blue underline font-size-14 nowrap">View all Categories</a>';
                        category_html += '               </span>';
                    }
                    category_html += '               </div>';
                    // End categories heading

                    // Start categories listing
                    if( categories.length ) {
                        category_html += '           <div class="width-100">';

                        for( var i = 0; i < categories.length; i++ ){
                            var category = categories[i];
                            category_html += '<a href="' + category.href + '&filter_name=' + searchinput.val() + '" class="inline-block margin-b10 underline-on-hover">';
                            category_html += '<span class="block colour-grey-darkest line-height-1pt1">' + category.name + '</span>';
                            category_html += '</a>';
                            category_html += '<br>';
                        }

                        // View x other categories link...
                        if( data.other_categories ){
                            category_html += '           <div class="margin-y5">';
                            category_html += '               <a href="https://www.memorycow.co.uk/index.php?route=product/search&category_id=' + $('#filter_category_id').val() + '&description=1&model=1&show_categories=1&keyword=' + encodeURIComponent(searchinput.val()) + '" class="colour-grey-darkest underline">+ ' + data.other_categories + ' more categories</a>';
                            category_html += '           </div>'
                        }

                        category_html += '           </div>';
                    } else {
                        category_html += '    <p class="margin-t10-negative font-size-18 colour-grey-darkest margin-0">Sorry, no matching categories were found.</p>';
                    }
                    // End categories listing

                    category_html += '     </div>';

                    // End categories

                    right_column_close += '               </div>';
                    right_column_close += '           </div>';

                    results_close += '       </div>';
                    results_close += '   </div>';
                    results_close += '</div>';

                    var html = '';
                    html = results_open + left_column_open;

                    // If we have products, show them on the left with devices and categories on the right
                    if( products.length ){
                        html += product_html + left_column_close + right_column_open;

                        // If we have devices, or we don't have categories, show devices at the top of the right column
                        if( devices.length || categories.length == 0 ){
                            html += device_html + category_html;
                        } else {
                            html += category_html + device_html;
                        }
                    }
                    // Else, we don't have products, so show devices or categories at the top of the left column
                    else {
                        // If we have any devices, show them at the top of the left column
                        if( devices.length || manufacturers.length || series.length ){
                            html += device_html + left_column_close + right_column_open;

                            if( categories.length ){
                                html += category_html;
                                html += '<hr class="margin-0" />';
                                html += product_html;
                            } else {
                                html += product_html;
                                html += '<hr class="margin-0" />';
                                html += category_html;
                            }
                        }
                        // Else, if we have categories, show them at the top of the left column
                        else if( categories.length ){
                            html += category_html + left_column_close + right_column_open + product_html;
                            html += '<hr class="margin-0" />';
                            html += device_html;
                        }
                        // Else, we have no results at all, so display as normal
                        else {
                            html += product_html + left_column_close + right_column_open + device_html + category_html;
                        }
                    }

                    html += right_column_close + results_close;

                    searchinput.next().html(html);

                    // Now, match the heights of the columns in the results for desktop screens.
                                        setTimeout( function(){
                        $.fn.matchHeight._apply( $('[data-mh="header-smartsearch"]') );
                    }, 250);

                                        // Custom GA Event tracking based on searches.
                    // Fire an event to Google Analytics after an interval to be sure that the search term doesn't
                    // change (i.e. ensure that if the user is a slow typer it only fires the event for the term once)
                    if( typeof( ga ) != 'undefined' ) {
                        clearTimeout(search_ga_event_wait);
                        search_ga_event_wait = setTimeout(function () {
                            // Send a 'Search' Searched event to analytics, including the search term
                            ga( 'send', 'event', 'Search', 'Searched', searchinput.val() );
                        }, 2000);
                    }
                                    }
            });
        }

        function hideSmartSearch() {
            $('.smartsearch').hide();
        }
    </script>
-->


