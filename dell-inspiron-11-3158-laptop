                <div class="row hide-medium-up text-center">
                    Page Ref Code: <strong>D110586</strong>
                </div>


                <div data-reveal id="trade-enquiry-modal"
                     class="reveal-modal trade-enquiry-modal callback-modal radius">
                    <div data-modal-content></div>
                </div>

            </main>
            <script>
                var LISTING_DYNAMIC_FILTER_URL = 'https://www.memorycow.co.uk/index.php?route=product/wizard/dynamic_filter&wizproduct_id=110586';
            </script>
            <script>
                var wizardSeries = {};
                var wizardModels = {};
            </script>
            <script>
                var wizardFilters = document.querySelectorAll('[data-wizard-filter]');
                var selectedFilters = {};

                var itemIsActive = function (currentItem, selectedFilterKeys) {
                    var defaultReturn = true;

                    for (var j = 0; j < selectedFilterKeys.length; j++) {
                        defaultReturn = false;

                        var currentFilterKey = selectedFilterKeys[j];
                        var currentFilterValues = Object.keys(selectedFilters[currentFilterKey]);

                        for (var k = 0; k < currentFilterValues.length; k++) {
                            var currentFilterValue = currentFilterValues[k];

                            if (currentItem && currentItem.filters && currentItem.filters[currentFilterKey] && currentItem.filters[currentFilterKey][currentFilterValue]) {
                                defaultReturn = true;
                            }
                        }

                        if (!defaultReturn) {
                            return false;
                        }
                    }

                    return defaultReturn;
                }

                var updateFilteredSeries = function () {
                    var wizardSeriesKeys = Object.keys(wizardSeries);
                    var selectedFilterKeys = Object.keys(selectedFilters);

                    for (var i = 0; i < wizardSeriesKeys.length; i++) {
                        var currentWizardKey = wizardSeriesKeys[i];
                        var currentWizardSeries = wizardSeries[currentWizardKey];
                        var $currentWizardSeriesEl = document.querySelector(`[data-wizard-series][data-title="${currentWizardKey.toLowerCase()}"]`);

                        if (!$currentWizardSeriesEl) {
                            continue;
                        }

                        if (itemIsActive(currentWizardSeries, selectedFilterKeys)) {
                            $currentWizardSeriesEl.classList.remove('none');
                        } else {
                            $currentWizardSeriesEl.classList.add('none');
                        }
                    }
                }

                var updateFilteredModels = function () {
                    var wizardModelsKeys = Object.keys(wizardModels);
                    var selectedFilterKeys = Object.keys(selectedFilters);

                    for (var i = 0; i < wizardModelsKeys.length; i++) {
                        var currentWizardKey = wizardModelsKeys[i];
                        var currentWizardModels = wizardModels[currentWizardKey];
                        var $currentWizardModelsEl = document.querySelector(`[data-wizard-model][data-title="${currentWizardKey.toLowerCase()}"]`);

                        if (!$currentWizardModelsEl) {
                            continue;
                        }

                        if (itemIsActive(currentWizardModels, selectedFilterKeys)) {
                            $currentWizardModelsEl.classList.remove('none');
                        } else {
                            $currentWizardModelsEl.classList.add('none');
                        }
                    }
                }

                wizardFilters.forEach(function (filter) {
                    filter.addEventListener('change', function (event) {
                        if (event.target.checked) {
                            if (!selectedFilters[event.target.name]) {
                                selectedFilters[event.target.name] = {};
                            }

                            selectedFilters[event.target.name][event.target.value] = true;
                        } else {
                            delete selectedFilters[event.target.name][event.target.value]

                            if (Object.keys(selectedFilters[event.target.name]).length === 0) {
                                delete selectedFilters[event.target.name];
                            }
                        }

                        updateFilteredSeries();
                        updateFilteredModels();
                    });
                });
            </script>


