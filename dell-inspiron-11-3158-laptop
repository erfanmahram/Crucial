<script type="text/javascript">

    //Array Intersect function used in productFilters();
    $.arrayIntersect = function (a, b) {
        return $.grep(a, function (i) {
            return $.inArray(i, b) > -1;
        });
    };

    // Price Slider
    if ($(".slider-wrap").length) {

        // Init Slider with custom limits
        $(".slider-wrap").noUiSlider({
            start: [5, $('#max_value').val()],
            step: 1,
            range: {
                'min': 5,
                'max': 813
            },
            connect: true,
            serialization: {
                lower: [
                    $.Link({target: $(".min-value")})
                ],
                upper: [
                    $.Link({target: $(".max-value")})
                ]
            }
        });

        // On change, update products
        $(".slider-wrap").on('set', filterProducts);

    }

    var wcIds = null;

    function resetFilters(evt) {
        evt.preventDefault();

        $('c').attr('checked', false);
        filterProducts();

        return false;
    }

    function filterProducts(addHist) {

        //ensures that if chrome wants to jump the user around the screen, it puts them back to where they once were
        /*
    var screenPosition = $(window).scrollTop();
    setTimeout(function(){
        if (screenPosition !== $(window).scrollTop()) {
            $('html,body').animate({
                scrollTop: screenPosition
            }, 5);
        }
    }, 5);
    */

        var min_value = parseFloat($(".min-value").html());
        var max_value = parseFloat($(".max-value").html());

        var urlArray = [];
        urlArray['priceFrom'] = min_value != parseFloat(5) ? min_value : '';
        urlArray['priceTo'] = max_value != parseFloat(813) ? max_value : '';
        urlArray['filter'] = [];
        urlArray['wcIds'] = [];

        if (typeof addHist === "undefined") {
            addHist = true;
        }

        // show all products and accordions (hide the ones we don't want below).
        $('.product-grid-item, .js-accordion').show();

        // Hide the no products message
        $('.js-no-products').hide();

        // Accordions ("Available Upgrades") - check the selected available wizard categories
        // If none are checked, show them all so never an empty page.
        if ($('.js-accordions-filter input:checked').length) {

            // Hide unchecked items
//        $('.js-accordions-filter input').not(':checked').each(function () {
//            $('.js-accordion[data-accordion-id="' + $(this).val() + '"]').hide();
//        });

            // Update URL
            $('.js-accordions-filter input:checked').each(function () {
                urlArray['wcIds'].push(parseInt($(this).data('wizcategory-id')));
            });

        }

        // Parse the selected filters
        var selected = new Array();
        if ($('.js-filter').length) {
            $('.js-filter').each(function () {
                var filter_group_id = parseInt($(this).data('filter-group-id'));
                selected[filter_group_id] = new Array();
                $(this).find('input:checked').each(function () {
                    selected[filter_group_id].push(parseInt($(this).val()));
                });
                selected[filter_group_id].sort();
                if (selected[filter_group_id].length == 0) {
                    delete selected[filter_group_id];
                }
            });
        }

        // Capacity (incase there isn't any filters so it will fall back)
        // If none are checked, show them all so never an empty page.
        if ($('.js-capacity-filter input:checked').length) {
            $('.js-capacity-filter input').not(':checked').each(function () {
                $('.product-grid-item[data-capacity="' + $(this).val() + '"]').hide();
            });
        }

        // Loop through each of the products for the filters and price
        $('.product-grid-item').each(function () {
            var product = $(this);

            // Price Slider
            var price = parseFloat(product.attr("data-price"));
            if (min_value && max_value && min_value < max_value) {
                if (price < min_value || price > max_value) {
                    if (price != 0) {
                        product.hide();
                    }
                }
            }

            // Filters
            if (selected.length) {
                selected.forEach(function (selected_filter_ids, filter_group_id) {
                    if (selected_filter_ids.length) {
                        var product_filter_ids = product.data('filter-' + filter_group_id);
                        if (typeof product_filter_ids !== 'undefined') {
                            var intersect = $.arrayIntersect(product_filter_ids, selected_filter_ids);
                            if (intersect.length == 0) {
                                product.hide();
                            } else {
                                urlArray['filter'].push(parseInt(intersect[0]));
                            }
                        } else {
                            product.hide();
                        }
                    }
                });
            }

        });

        // Hide empty accordions (to tidy up)
        $('.js-accordion').each(function () {
            if ($('.product-grid-item:visible', this).length == 0) {
                $(this).hide();
            }
        });

        urlArray['filter'].filter(Number); //ensure all only numbers
        urlArray['filter'] = $.grep(urlArray['filter'], function (v, k) {
            return $.inArray(v, urlArray['filter']) === k;
        }); //array unique
        urlArray['filter'].sort();

        if (wcIds == null) {
            wcIds = urlArray['wcIds'];
        }

        //added for filter URLs
        if (addHist) {
            var qStr = arrayToQueryString(urlArray);
            wcIds.sort();
            urlArray['wcIds'].sort();

            // Reload page if new wizard category
            if (JSON.stringify(wcIds) != JSON.stringify(urlArray['wcIds'])) {
                if (qStr) {
                    window.location = 'https://www.memorycow.co.uk/laptop/dell/inspiron-11-3000-series/dell-inspiron-11-3158-laptop?' + qStr;
                } else {
                    window.location = 'https://www.memorycow.co.uk/laptop/dell/inspiron-11-3000-series/dell-inspiron-11-3158-laptop';
                }
            }

            // Update URL if standard filter
            if (qStr) {
                history.pushState('reload', null, 'https://www.memorycow.co.uk/laptop/dell/inspiron-11-3000-series/dell-inspiron-11-3158-laptop?' + qStr);
            } else {
                history.pushState('reload', null, 'https://www.memorycow.co.uk/laptop/dell/inspiron-11-3000-series/dell-inspiron-11-3158-laptop');
            }

        }

        // Update the 'Available Upgrade' links with the new filters
        $('[data-accordion-url]').each(function () {
            var this_urlArray = [];
            for (var i in urlArray) {
                this_urlArray[i] = urlArray[i];
            }
            var url = $(this).attr('href');
            var url_parts = url.split('?');

            if (typeof (url_parts[1]) != 'undefined') {
                var url_params = url_parts[1].split('&');

                for (var j in url_params) {
                    var param_parts = url_params[j].split('=');
                    var key = param_parts[0];
                    var param = typeof (param_parts[1]) ? param_parts[1] : '';

                    if (key == 'wizcategory_id' && param != '') {
                        this_urlArray['wizcategory_id'] = param;
                    }
                }
            }

            var this_qStr = arrayToQueryString(this_urlArray);
            $(this).attr('href', url_parts[0] + '?' + this_qStr);
        });

        // If no products are shown, show the no product message
        if ($('.product-grid-item:visible').length == 0 || $('.js-accordion:visible').length == 0) {
            $('.js-no-products').show();
        }

    }

    // Generate URL query
    function arrayToQueryString(array_in) {
        var out = new Array();
        for (var key in array_in) {
            var val = '';
            if (Array.isArray(array_in[key])) {
                val = array_in[key].join();
            } else {
                val = encodeURIComponent(array_in[key]);
            }
            if (key.length && val.length) {
                out.push(key + '=' + val);
            }
        }
        return out.join('&');
    }

    // If query requires a refresh, reload the page
    window.addEventListener('popstate', function (event) {
        if (event.state === 'reload') {
            location.reload();
        }
    });

    // Filter on change of checkboxes
    $('.js-accordions-filter input, .js-filter input, .js-capacity-filter input').on('change', filterProducts);

    // Filter on pageload
    $(filterProducts(false));

    $(document).on('click', '[data-reset-filters]', resetFilters);

    // Filter More/Less
    function filterMoreLessToggle() {
        var $button = $(this);
        var $filter_list = $(this).parents('[data-filter-list]').first();

        if (typeof ($filter_list.data('open')) != 'undefined' && $filter_list.data('open')) {
            $filter_list.find('[data-hidden-filter]').addClass('none');
            $button.text($button.data('more-label'));
            $filter_list.data('open', false);
        } else {
            $filter_list.find('[data-hidden-filter]').removeClass('none');
            $button.text($button.data('less-label'));
            $filter_list.data('open', true);
        }
    }

    $(document).on('click', '[data-filter-more-less-toggle]', filterMoreLessToggle);

</script>
<script type="text/javascript"><!--
$(document).ready(function () {

    $('#wizcategory_id_device').change(function () {

        $('.js-submit-wizard').attr('disabled', true);
        $('.js-submit-wizard').removeClass('colour-orange').addClass('colour-grey');

        // Show loading overlay (some searches take some time)
        $('.js-wizard-loading-overlay').removeClass('none');

        resetSelect('#manufacturer_id_device');
        resetSelect('#wizproduct_id_device');
        resetSelect('#wiztype_id_device');
        resetSelect('#configurator_series_device');

        $.ajax({
            type: 'POST',
            url: 'index.php?route=product/wizard/getJsonDevice',
            data: 'wizcategory_id=' + encodeURIComponent($('#wizcategory_id_device').val()),
            dataType: 'json',
            success: function (data) {

                if (data.devices.length) {
                    $.each(data.devices, function (k, val) {
                        $('#wiztype_id_device')
                            .append($('<option>', {value: val.wiztype_id})
                                .text(val.name));
                    });
                    $('#wiztype_id_device').attr('disabled', false);
                }

            },
            complete: function () {

                // Hide loading overlay
                $('.js-wizard-loading-overlay').addClass('none');

            }
        });
        return false;
    });

    $('#wiztype_id_device').change(function () {

        $('.js-submit-wizard').attr('disabled', true);
        $('.js-submit-wizard').removeClass('colour-orange').addClass('colour-grey');

        // Show loading overlay (some searches take some time)
        $('.js-wizard-loading-overlay').removeClass('none');

        resetSelect('#manufacturer_id_device');
        resetSelect('#wizproduct_id_device');
        resetSelect('#configurator_series_device');

        $.post(
            'index.php?route=product/wizard/getJsonBrands',
            $('.js-kmc-wizard').serialize(),
            function (data) {

                if (data.manufacturers.length) {
                    $.each(data.manufacturers, function (k, val) {
                        $('#manufacturer_id_device')
                            .append($('<option>', {value: val.manufacturer_id})
                                .text(val.name));
                    });
                    $('#manufacturer_id_device option:first').removeAttr('disabled').attr('selected', 'selected');
                    $('#manufacturer_id_device').val('').removeAttr('disabled');//enable the dropdown
                    //check for one value (2 because the label is a value) and trigger a change on it if only one
                    if ($('#manufacturer_id_device option').length == 2) {
                        $('#manufacturer_id_device option:last').attr('selected', 'selected');
                        $('#manufacturer_id_device').trigger('change');
                    }
                    ;
                }

                // Hide loading overlay
                $('.js-wizard-loading-overlay').addClass('none');

            },
            'json'
        );

    });

    $('#manufacturer_id_device').change(function () {

        $('.js-submit-wizard').attr('disabled', true);
        $('.js-submit-wizard').removeClass('colour-orange').addClass('colour-grey');

        // Show loading overlay (some searches take some time)
        $('.js-wizard-loading-overlay').removeClass('none');

        resetSelect('#wizproduct_id_device');
        resetSelect('#configurator_series_device');

        $.post(
            'index.php?route=product/wizard/getJsonModels',
            $('.js-kmc-wizard').serialize(),
            function (data) {
                if (data.configurator_series.length) {
                    $.each(data.configurator_series, function (k, val) {
                        $('#configurator_series_device')
                            .append($('<option>', {value: val.keyword})
                                .text(val.name));
                    });
                    $('#configurator_series_device option:first').removeAttr('disabled').attr('selected', 'selected');
                    $('#configurator_series_device').val('').removeAttr('disabled');//enable the dropdown
                    //check for one value (2 because the label is a value) and trigger a change on it if only one
                    if ($('#js-wiz-manufacturer-device select option').length == 1 && $('#configurator_series_device option').length == 2) {
                        $('#configurator_series_device option:last').attr('selected', 'selected');
                        $('#configurator_series_device').trigger('change');
                    }
                    ;
                }
                if (data.models.length) {
                    $.each(data.models, function (k, val) {
                        $('#wizproduct_id_device')
                            .append($('<option>', {value: val.href})
                                .text(val.name));
                    });
                    $('#wizproduct_id_device option:first').removeAttr('disabled').attr('selected', 'selected');
                    $('#wizproduct_id_device').val('').removeAttr('disabled');//enable the dropdown
                    //check for one value (2 because the label is a value) and trigger a change on it if only one
                    if ($('#wizproduct_id_device option').length == 2) {
                        $('#wizproduct_id_device option:last').attr('selected', 'selected');
                        $('#wizproduct_id_device').trigger('change');
                    }
                    ;
                }

                // Hide loading overlay
                $('.js-wizard-loading-overlay').addClass('none');

            },
            'json'
        );

    });

    $('#configurator_series_device').change(function () {

        $('.js-submit-wizard').attr('disabled', true);
        $('.js-submit-wizard').removeClass('colour-orange').addClass('colour-grey');

        // Show loading overlay (some searches take some time)
        $('.js-wizard-loading-overlay').removeClass('none');

        resetSelect('#wizproduct_id_device');

        $.post(
            'index.php?route=product/wizard/getJsonModels',
            $('.js-kmc-wizard').serialize(),
            function (data) {
                $(".js-wizard-loading").hide();
                if (data.models.length) {
                    $.each(data.models, function (k, val) {
                        $('#wizproduct_id_device')
                            .append($('<option>', {value: val.href})
                                .text(val.name));
                    });
                    $('#wizproduct_id_device option:first').removeAttr('disabled').attr('selected', 'selected');
                    $('#wizproduct_id_device').val('').removeAttr('disabled');//enable the dropdown
                    //check for one value (2 because the label is a value) and trigger a change on it if only one
                    if ($('#wizproduct_id_device option').length == 2) {
                        $('#wizproduct_id_device option:last').attr('selected', 'selected');
                        $('#wizproduct_id_device').trigger('change');
                    }
                    ;
                }

                // Hide loading overlay
                $('.js-wizard-loading-overlay').addClass('none');

            },
            'json'
        );

    });

    $('#wizproduct_id_device').change(function () {
        if ($(this).val() != '') {
            $('.js-submit-wizard').attr('disabled', false);
            $('.js-submit-wizard').addClass('colour-orange').removeClass('colour-grey');
        } else {
            $('.js-submit-wizard').attr('disabled', true);
            $('.js-submit-wizard').removeClass('colour-orange').addClass('colour-grey');
        }
    });
    $('#wizproduct_id_device').change();

    $('.js-submit-wizard').click(function (e) {
        e.preventDefault();
        if ($('#wizproduct_id_device').val()) {
            location.href = $('#wizproduct_id_device').val();
        }
    });

    $('.js-reset-wizard').click(function (e) {
        e.preventDefault();

        $('.js-submit-wizard').attr('disabled', true);
        $('.js-submit-wizard').removeClass('colour-orange');
        $('.js-submit-wizard').addClass('colour-grey');

        resetSelect('#manufacturer_id_device');
        resetSelect('#wizproduct_id_device');
        resetSelect('#wiztype_id_device');
        resetSelect('#configurator_series_device');

        $('#wizcategory_id_device').val('');
        $('#wizcategory_id_device').change();

    });

});

function resetSelect(select) {
    $(select)
        .attr('disabled', 'disabled')
        .children('option')
        .first()
        .attr('selected', 'selected')
        .siblings()
        .remove();
}

