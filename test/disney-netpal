                                    <div class="input-group plus-minus-input">
                                        <div class="input-group-button">
                                            <button type="button"
                                                onclick="javascript:micron.productdetail.components.productdetail.subtractItem('{{sku}}');"
                                                class="button qty-button" data-quantity="minus" data-field="quantity" tabindex="-1">
                                                <i class="icon-minus" aria-hidden="true" tabindex="0"></i>
                                            </button>
                                        </div>
                                        <input class="input-group-field qty-input" type="number" name="quantity"
                                            id="quantity_{{sku}}" value="1"
                                            onblur="javascript:micron.productdetail.components.productdetail.qtyValidation('{{sku}}');">
                                        <div class="input-group-button">
                                            <button type="button" class="button qty-button" data-quantity="plus"
                                                data-field="quantity" tabindex="-1"
                                                onclick="javascript:micron.productdetail.components.productdetail.addItem('{{sku}}');">
                                                <i class="icon-plus" aria-hidden="true" tabindex="0"></i>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                                <a class="button disabled secondary expanded" tabindex="0"
                                    id="cartBtn-{{sku}}" data-pagepath="{{../page-path}}">Checking inventory</a>
                            {{/ifCond}}
                        {{/ifCond}}
                        <div class="text-center hollow">
                            <!--/* where to buy button */-->
                            {{#ifCond ../prod-type '!==' 'accessories'}}
                                {{#ifCond show-wtb '===' 'true'}}
                                    <!--/*wtb accessories */-->
                                    {{#isNotEmpty wtb-url}}
                                        <a class="button-wrap button secondary expanded" href="{{wtb-url}}">Where to buy</a>
                                    {{/isNotEmpty}}
                                    {{#isEmpty wtb-url}}
                                        {{#if ../wtb-asset-id}}
                                            <button class="cswidget button secondary expanded"
                                                data-asset-id="{{../wtb-asset-id}}" type="button"
                                                pixel-id="wtb" data-product-sku="{{sku}}">
                                                Where to buy
                                            </button>
                                        {{/if}}
                                    {{/isEmpty}}
                                {{/ifCond}}
                            <!--/* end wtb accessories */-->
                            {{/ifCond}}
                        </div>
                     </div>
                </div>
            </div>
        {{/each}}
</script>

                            
