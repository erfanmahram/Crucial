        <script>
            function initializePowerReview() {
                var items = [];
                for (n = 0; n < pr_review_items.length; n++) {
                    var compId = pr_review_items[n];
                    var pageId = compId;
                    var splitIndex = compId.indexOf('___');
                    if (splitIndex > 0) {
                        pageId = compId.substring(0, splitIndex);
                    }
                    items.push({
                        api_key: 'a73fc3f3-3fb3-464a-adfa-5a51c20be68b',
                        locale: 'en_US',
                        merchant_group_id: '10419',
                        merchant_id: '9950',
                        page_id: pageId,
                        review_wrapper_url: '/write-review?oid=' + pageId,
                        REVIEW_DISPLAY_SNAPSHOT_TYPE: 'SIMPLE',
                        components: {
                            CategorySnippet: 'review-snippet-' + compId
                        }
                    });
                }

                if (items.length > 0) {
                    var text = JSON.stringify(items);
                    POWERREVIEWS.display.render(JSON.parse(text));
                }
                removeReviewItems();
            }
        </script>
    

      
      





      
      



    

    
        




